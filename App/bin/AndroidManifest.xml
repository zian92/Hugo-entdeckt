<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.hugo.hugo_entdeckt"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="de.hugo.hugo_entdeckt.SplashScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="de.hugo.hugo_entdeckt.LoginActivity"
            android:label="Login"
            android:noHistory="true" >
        </activity>
        <activity
            android:name="de.hugo.hugo_entdeckt.HomeScreenActivity"
            android:label="HomeScreen" >
        </activity>
        <activity
            android:name="de.hugo.hugo_entdeckt.QuizActivity"
            android:label="Quiz" >
        </activity>
    </application>

</manifest>